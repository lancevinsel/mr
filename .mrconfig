[DEFAULT]
git_gc = git gc "$@"
jobs = 5

include = cat /usr/share/mr/vcsh

include = cat ~/.config/mr/config.d/*

[.vim/bundle/nerdtree]
checkout = git clone 'git://github.com/scrooloose/nerdtree.git' 'nerdtree'

[.vim/bundle/nerdtree-git-plugin]
checkout = git clone 'https://github.com/Xuyuanp/nerdtree-git-plugin' 'nerdtree-git-plugin'

[.vim/bundle/sparkup]
checkout = git clone 'https://github.com/rstacruz/sparkup' 'sparkup'

[.vim/bundle/vim-colors-solarized]
checkout = git clone 'git://github.com/altercation/vim-colors-solarized.git' 'vim-colors-solarized'

[.vim/bundle/vim-easymotion]
checkout = git clone 'https://github.com/easymotion/vim-easymotion' 'vim-easymotion'

[.vim/bundle/vim-surround]
checkout = git clone 'https://github.com/tpope/vim-surround' 'vim-surround'

[.oh-my-zsh]
checkout = git clone 'https://github.com/robbyrussell/oh-my-zsh.git' '.oh-my-zsh'

[.vifm/colors]
checkout = git clone 'https://github.com/vifm/vifm-colors' 'colors'

[.irssi/scripts.irssi.org.git]
checkout = git clone 'https://github.com/irssi/scripts.irssi.org.git' 'scripts.irssi.org.git'
